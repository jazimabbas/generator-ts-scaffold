{
  "name": "@jazim/generator-ts-scaffold",
  "version": "1.0.0",
  "description": "TypeScript scaffolding for Yeoman generators",
  "type": "module",
  "exports": {
    "./generators/app": "./lib/generators/app/index.js"
  },
  "files": [
    "lib",
    "src/generators/app/templates"
  ],
  "scripts": {
    "build": "tsc && cpx \"src/generators/**/templates/**/*\" lib/generators",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "yeoman-generator"
  ],
  "author": "Jazim Abbas",
  "homepage": "https://github.com/jazimabbas/generator-ts-scaffold",
  "repository": {
    "url": "https://github.com/jazimabbas/generator-ts-scaffold"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "yeoman-generator": "^7.5.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.8",
    "@types/node": "^24.0.14",
    "cpx": "^1.5.0",
    "typescript": "^5.8.3"
  }
}
